{
  "name": "firemyna",
  "version": "0.35.0",
  "description": "Firebase application framework",
  "main": "lib/index.js",
  "bin": {
    "firemyna": "bin/run"
  },
  "files": [
    "lib",
    "bin",
    "oclif.manifest.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf lib && tsc -p tsconfig.lib.json",
    "prepack": "npm run build && oclif manifest",
    "postpack": "rm -f oclif.manifest.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firemyna/firemyna.git"
  },
  "keywords": [
    "Firebase",
    "Firebase Functions",
    "Functions Hosting"
  ],
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firemyna/firemyna/issues"
  },
  "homepage": "https://github.com/firemyna/firemyna#readme",
  "devDependencies": {
    "@types/estree": "0.0.50",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.2",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.15.7",
    "jest": "^27.3.1",
    "oclif": "^2.6.3",
    "prettier": "^2.8.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsm": "^2.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@esbuild-plugins/node-resolve": "^0.1.4",
    "@oclif/core": "^1.6.4",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-version": "^1.0.4",
    "@types/inquirer": "^8.1.3",
    "acorn": "^8.5.0",
    "chokidar": "^3.5.2",
    "cli-ux": "^5.6.3",
    "commander": "^8.3.0",
    "esbuild": "^0.16.17",
    "estree-walker": "^2.0.2",
    "glob": "^7.2.0",
    "inquirer": "^8.2.0",
    "js-fns": "^2.5.1",
    "picocolors": "^1.0.0"
  },
  "oclif": {
    "commands": "./lib/cli/commands",
    "bin": "firemyna",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  }
}
